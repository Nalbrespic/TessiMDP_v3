{
    "name": " tms/tessimd-portail",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/",
            "BeSimple\\": "src/"

        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "files": [
            "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
        ]
    },

    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Tessi-Tms/TmsLogisticBundle.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Tessi-Tms/TmsRestBundle.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Tessi-Tms/TmsRestClientBundle"
        }
    ],

    "require": {
        "php": ">=5.5.9",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "incenteev/composer-parameter-handler": "^2.0",
        "sensio/distribution-bundle": "^5.0.19",
        "sensio/framework-extra-bundle": "^5.0.0",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/swiftmailer-bundle": "^2.6.4",
        "symfony/symfony": "3.4.*",
        "twig/twig": "^1.0||^2.0",

        "jms/serializer-bundle":  "2.2.*",
        "paragonie/random_compat": "1.*",
        "phpoffice/phpspreadsheet": "1.*",
        "tms/logistic-bundle": "dev-master",
        "mk-j/php_xlsxwriter": "0.*",
        "cmen/google-charts-bundle": "3.*"
    },
    "require-dev": {
        "sensio/generator-bundle": "~3.0",
        "symfony/phpunit-bridge": "~2.7"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"

        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true,
        "github-oauth": {
            "github.com": "29b3721ad5b3b7ad0849eddb4045666c0c66be8d"
        }
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": null
    }
}
